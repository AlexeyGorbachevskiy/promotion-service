    "target": "es6",
    "module": "commonjs",
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./",
    "paths": {
      "assets/*": ["src/assets/*"],
      "helpers/*": ["src/helpers/*"],
      "ducks/*": ["src/ducks/*"],
      "constants/*": ["src/constants/*"],
      "mocked-backend": ["src/mocked-backend"],
      "hooks": ["src/hooks"],
      "styles": ["src/styles"],
      "basics/*": ["src/web/basics/*"],
      "components/*": ["src/web/components/*"],
      "pages/*": ["src/web/pages/*"]
    }
  }
}
